{
  "name": "architecture-hexagonal-node",
  "version": "1.0.0",
  "description": "",
  "main": "build/src/start.js",
  "scripts": {
    "postinstall": "pnpm run db:generate && pnpm run db:push",
    "dev": "concurrently \"ts-node-dev --no-notify --respawn --transpile-only src/start.ts\"",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "vitest",
    "build": "pnpm run clean:build && tsc",
    "start": "node dist/src/start.js",
    "docs": "tsoa spec",
    "clean:build": "rimraf dist public",
    "clean:install": "rimraf node_modules package-lock.json yarn.lock pnpm-lock.yaml bun.lockb",
    "clean": "pnpm run clean:build && pnpm run clean:install",
    "db:init": " npx prisma init --datasource-provider sqlite",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:deploy": "npx prisma migrate deploy",
    "db:push": "npx prisma db push",
    "db:studio": "npx prisma studio",
    "db:seed": "ts-node seed/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "5.7.1",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "glob": "10.3.10",
    "helmet": "7.1.0",
    "http-status": "1.7.3",
    "inversify": "6.0.2",
    "morgan": "1.10.0",
    "prisma": "5.7.1",
    "reflect-metadata": "0.2.1",
    "rimraf": "5.0.5",
    "swagger-ui-express": "5.0.0",
    "tsoa": "5.1.1"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.11",
    "@types/morgan": "1.9.9",
    "@types/node": "20.10.5",
    "@types/swagger-ui-express": "4.1.6",
    "@typescript-eslint/eslint-plugin": "6.15.0",
    "concurrently": "8.2.2",
    "env-var": "7.4.1",
    "eslint": "8.56.0",
    "eslint-config-standard-with-typescript": "43.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.4.0",
    "eslint-plugin-promise": "6.1.1",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.3.3",
    "vitest": "1.1.0"
  }
}
